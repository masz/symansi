user www-data;
worker_processes 4;
pid /var/run/nginx.pid;

events {
    worker_connections 2048;
    multi_accept on;
    use epoll;
}

http {
    server_tokens off;
    sendfile on;
    tcp_nopush on;
    tcp_nodelay on;
    keepalive_timeout 15;
    types_hash_max_size 2048;
    include /etc/nginx/mime.types;
    default_type applicaiton/octet-stream;
    access_log off;
    error_log off;
    gzip on;
    include /etc/nginx/conf.d/*.conf;
}
